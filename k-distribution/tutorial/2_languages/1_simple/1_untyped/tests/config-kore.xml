<tests>

  <test definition="simple-untyped"
        programs="."
        extension="simple"
        exclude="dekker exceptions_07 threads_07 threads_10 threads_11"
        skip="pdf"
        results="." >
        <all-programs>
          <krun-option name="--parser" value="kast --sort Stmts" />
          <krun-option name="--output" value="none" />
        </all-programs>
        <program name="threads_05.simple">
          <krun-option name="--parser" value="kast --sort Stmts" />
          <krun-option name="--search" />
          <krun-option name="--bound" value="5" />
        </program>
        <program name="exceptions_07.simple">
          <krun-option name="--parser" value="kast --sort Stmts" />
          <krun-option name="--search" />
          <krun-option name="--pattern" value="&lt;out&gt; ListItem(#buffer(S:String)) &lt;/out&gt;" />
        </program>
  </test>

</tests>
